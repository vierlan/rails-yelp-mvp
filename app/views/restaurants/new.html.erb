<h1>Please enter restaurant details</h1>


<div class="container">
  <%= simple_form_for(@restaurant) do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.select :category, :collection => ["chinese", "italian", "japanese", "french", "belgian"] %>
    <%= f.submit %>
  <% end %>
</div>
